<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

    <Frame name="GW5SRTemp" virtual="true">
        <Size x="316" y="5"></Size>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture name="$parentBackground" file="Interface\AddOns\GW2_UI\textures\gwstatusbar-bg">
                    <Size x="316" y="20"></Size>
                </Texture>
            </Layer>
            <Layer level="BORDER">
                <Texture parentKey="bar" file="Interface\AddOns\GW2_UI\textures\gwstatusbar">
                    <Size x="316" y="1"/>
                    <Anchors>
                        <Anchor point="LEFT" x="0" y="0"/>
                        <Anchor point="RIGHT" relativePoint="LEFT" x="0" y="0"/>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
        <Frames>
            <StatusBar parentKey="statusBar" name="$parentBar" defaultValue="1" minValue="0" maxValue="1">
                <Size x="315" y="1"/>
                <Anchors>
                    <Anchor point="LEFT" x="0" y="0"/>
                </Anchors>
                <BarTexture file="Interface\AddOns\GW2_UI\textures\gwstatusbar"/>
                <Layers>
                    <Layer level="OVERLAY">
                        <FontString parentKey="label" name="$parentString" fontSize="8" text="" fontColor="white" inherits="GameFontNormalSmall" justifyH="CENTER">
                            <Color r="1" b="1" g="1"></Color>
                            <Size x="316" y="16"/>
                            <Anchors>
                                <Anchor point="CENTER" x="0" y="0"/>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
            </StatusBar>
            <Frame parentKey="shapeshiftformHelper" hidden="true"/>
        </Frames>
    </Frame>

</Ui>